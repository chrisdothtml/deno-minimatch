{
  "private": true,
  "engines": {
    "node": ">=11",
    "yarn": ">=1.12"
  },
  "scripts": {
    "build:bundle": "rollup -c",
    "build:ts-fix": "terser --compress -- index.js -o index.js -b \"beautify=false,preamble='//@ts-ignore'\"",
    "build": "yarn build:bundle && yarn build:ts-fix",
    "lint": "standard --verbose | snazzy",
    "test": "yarn lint && deno index.test.js"
  },
  "standard": {
    "ignore": [
      "index.js"
    ]
  },
  "devDependencies": {
    "minimatch": "^3.0.4",
    "rollup": "^0.68.1",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "terser": "^3.13.1"
  }
}
